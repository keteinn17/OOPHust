<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.DigitalVideoDiscMapper">
    <resultMap id="DVD" type="org.example.DigitalVideoDisc">
        <result column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="category" property="category"/>
        <result column="director" property="director"/>
        <result column="length" property="length"/>
        <result column="cost" property="cost"/>
    </resultMap>

    <select id="list" resultMap="DVD">
        SELECT *
        FROM digitalvideodisc
    </select>

    <insert id="insert" parameterType="org.example.DigitalVideoDisc">
        INSERT INTO digitalvideodisc
        (
         title,
         category,
         director,
         length,
         cost
        )
        VALUES
        (
         #{title},
        #{category},
         #{director},
         #{length},
         #{cost}
        )
    </insert>
</mapper>